//  comparison operators
bool operator==( const vector<T>& rhs )   { return lhs.ivector == rhs.ivector }

bool operator!=( const vector<T>& rhs )   { return lhs.ivector != rhs.ivector }

bool operator< ( const vector<T>& rhs )   { return lhs.ivector < rhs.ivector }

bool operator<=( const vector<T>& rhs )   { return lhs.ivector <= rhs.ivector }

bool operator> ( const vector<T>& rhs )   { return lhs.ivector > rhs.ivector }

bool operator>=( const vector<T>& rhs )   { return lhs.ivector >= rhs.ivector }

